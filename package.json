{
  "name": "dc-app",
  "version": "0.2.0",
  "description": "Data Commandr: Integrate. Transform. Analyze.",
  "keywords": ["Data Commandr"],
  "author": "Alexandr Savinov",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf node_modules doc dist && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean-install && npm start",
    "watch": "webpack --watch --progress --profile",
    "build": "rimraf dist && webpack --progress --profile --bail",
    "server": "webpack-dev-server --inline --progress --port 8080",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "lint": "tslint --force \"src/**/*.ts\"",
    "e2e": "protractor",
    "e2e-live": "protractor --elementExplorer",
    "pretest": "npm run lint",
    "test": "karma start",
    "test-watch": "karma start --no-single-run --auto-watch",
    "ci": "npm run e2e && npm run test",
    "docs": "typedoc --options typedoc.json src/app/app.component.ts",
    "start": "npm run server",
    "postinstall": "npm run webdriver-update && typings install"
  },
  "dependencies": {
    "@angular/common": "~2.1.2",
    "@angular/compiler": "~2.1.2",
    "@angular/core": "~2.1.2",
    "@angular/forms": "~2.1.2",
    "@angular/http": "~2.1.2",
    "@angular/platform-browser": "~2.1.2",
    "@angular/platform-browser-dynamic": "~2.1.2",

    "@angular/router": "~3.1.2",

    "es6-shim": "^0.35.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-rc.3",

    "zone.js": "^0.6.26",

    "ng2-bootstrap": "1.1.16",
    "bootstrap": "^4.0.0-alpha.5",
    "font-awesome": "4.7.0",
    "ng2-toastr": "^1.3.1"
	},
  "devDependencies": {

    "typescript": "^2.0.10",
    "ts-helpers": "^1.1.2",
    "tslint": "^3.15.1",

    "node-sass": "^3.13.0",
    "phantomjs-prebuilt": "^2.1.13",
    "protractor": "^4.0.11",
    "remap-istanbul": "^0.7.0",
    "rimraf": "^2.5.4",
    "shelljs": "^0.7.5",
    "less": "^2.7.1",
    "typedoc": "^0.5.1",

    "autoprefixer": "^6.5.3",
    "codelyzer": "1.0.0-beta.4",


    "typings": "^2.0.0",
    "@types/es6-shim": "^0.31.32",
    "@types/node": "^6.0.48",
    "@types/jasmine": "^2.5.38",


    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",

    "copy-webpack-plugin": "^4.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.24.1",

    "angular2-template-loader": "^0.6.0",
    "css-loader": "^0.26.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "istanbul-instrumenter-loader": "^1.0.0",
    "json-loader": "^0.5.4",
    "karma-sourcemap-loader": "^0.3.7",
    "null-loader": "0.1.1",
    "postcss-loader": "^1.1.1",
    "raw-loader": "0.5.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "less-loader": "2.2.3",
    "ts-loader": "^1.2.2",
    "tslint-loader": "^2.1.5",
    "url-loader": "^0.5.7",


    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "^2.7.0",

    "karma": "1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-istanbul": "0.2.1",
    "karma-webpack": "1.8.0"
  }
}
